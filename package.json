{
  "name": "@lostcities-cloud/lostcities-frontend",
  "version": "0.1.0",
  "private": false,
  "repository": "https://github.com/lostcities-cloud/lostcities-frontend",
  "description": "",
  "scripts": {
    "serve": "vue-cli-service serve --config vue.config.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run lint && npm run unit",
    "unit": "jest",
    "pack": "cp package.json ./dist && npm pack ./dist && mv lostcities-cloud-lostcities-frontend-*.tgz ./dist/",
    "build-docker": "docker build . -t ghcr.io/lostcities-cloud/lostcities-frontend",
    "push-docker": "docker push ghcr.io/lostcities-cloud/lostcities-frontend"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/compat": "^3.1.0",
    "axios": "1.4.0",
    "bootstrap": "4.5.3",
    "bootstrap-vue": "^2.23.0",
    "net": "^1.0.2",
    "node-sass": "^7.0.1",
    "sockjs-client": "1.5.2",
    "stompjs": "^2.3.3",
    "vue": "^3.1.0",
    "@kyvg/vue3-notification": "2.9.1",
    "vue-router": "4.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^28",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^28",
    "jest-serializer-vue": "^0.2.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "vue-loader": "17.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-debugger": 1,
      "vue/multi-word-component-names": "off"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "requireConfigFile": false,
      "ignorePatterns": "**/*.js"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "/opt/node/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "/opt/node/node_modules/babel-jest",
      ".*\\.(vue)$": "/opt/node/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "/opt/node/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "keywords": []
}
